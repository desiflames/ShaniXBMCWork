        this.playerTimeOut = true;
        this.sendPlaybackResponse(com.jadoo.as2.JadooPlus.Utils.ErrorCodes.WMV_REACHED_MAX_BUF_TIME);
        this.stopWmv();
    }

    function onVodEvent()
    {
