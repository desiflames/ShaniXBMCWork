    function displayButtons()
    {
        var __reg8 = 50;
        var __reg5 = 0;
        while (__reg5 < this.buttons.length) 
        {
            var __reg2 = this.disclaimerBaseMC.createEmptyMovieClip("btnBaseMC" + __reg5, this.disclaimerBaseMC.getNextHighestDepth());
