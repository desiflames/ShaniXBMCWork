        __reg2._y = 560;
        __reg2._visible = false;
        var __reg4 = this.infoBaseMC.createTextField("listingText", this.infoBaseMC.getNextHighestDepth(), 1125, __reg2._y - 6, 0, 0);
        this.setTxtFormat("Arial", "right", 23, false, 16777215);
