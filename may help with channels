            this.trackerXml = new XML();
            this.trackerXml.onHTTPStatus = this.fn.onTrackerHttpResponse;
            this.trackerXml.load(__reg3);
            this.trackerTimerId = setInterval(this, "onTrackerTimeOut", 5000);
            __reg3 = null;
        }
        else if (__reg0 === "rtmp-live") 
        {
            if (__reg2 == "hls-live") 
