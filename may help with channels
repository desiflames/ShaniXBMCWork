        {
            this.tierItems = __reg3;
            this.topItemIdx = 0;
            this.bottomItemIdx = 0 + (this.maxItems - 1);
        }
        else if (this.currentItemIdx > this.previousItemIdx) 
        {
            ++this.topItemIdx;
            ++this.bottomItemIdx;
            this.tierItems.shift();
            this.tierItems.push(__reg3[0]);
        }
        else if (this.currentItemIdx < this.previousItemIdx) 
