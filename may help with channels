    function getItemStartIdx()
    {
        if (this.itemType == "dailymotionplaylist") 
        {
            return 0;
        }
        return Math.floor(this.currentItemIdx / this.maxItems) * this.maxItems;
    }

    function getCurrentItemIdx()
    {
        if (this.itemType == "dailymotionplaylist") 
        {
