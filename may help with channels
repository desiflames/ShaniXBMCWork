        }
        if (this.startIdsPool != null) 
        {
            delete this.startIdsPool;
            this.startIdsPool = null;
        }
        if (this.onPoolEvent != null) 
        {
            delete (this.onPoolEvent);
            this.onPoolEvent = null;
        }
        this.passItemsToTier = null;
        this.parseTierItems = null;
        this.itemsXml = null;
    }

}

