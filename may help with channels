    function onEnterKey()
    {
        var __reg5 = arguments;
        if ((__reg0 = this.contextMenu[this.contextMenu.length - 1]) === "MainMenu") 
        {
            var __reg3 = __reg5[0];
            if (__reg3.name == "Unsubscribe Selected Item") 
