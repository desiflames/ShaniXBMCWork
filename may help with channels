                        this.wasPopupEnabled = false;
                        this.isChannelItem = false;
                        this.epg.enable();
                        this.epg.displayMessage(this.netErrorStr);
                    }
                }
                else 
                {
                    this.wasPopupEnabled = false;
                    this.isChannelItem = false;
                    this.epg.enable();
                }
            }
            else 
            {
                this.wasPopupEnabled = false;
                this.isChannelItem = false;
                this.epg.enable();
            }
        }
        else 
        {
            this.isEnabled = true;
            if (__reg2.popUpType == "non-blocking") 
            {
