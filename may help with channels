                        }
                    }
                }
                else if (button == "Cancel") 
                {
                    this.adEnabled = false;
                    this.wasPopupEnabled = false;
                    this.isEnabled = true;
                    if (this.viewBaseMC._visible == false) 
                    {
                        this.enableView();
                    }
                    this.enable();
                    this.onTierEvent.notify("onRemovePopup");
                }
            }
            else 
            {
                this.adEnabled = false;
                this.wasPopupEnabled = false;
                this.isEnabled = true;
                if (this.viewBaseMC._visible == false) 
