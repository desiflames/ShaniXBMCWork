                }
            }
            else 
            {
                this.testFailMC._visible = true;
                this.displayText("Check your Internet Connectivity", this.txtStartX, this.txtStartY + this.pageConfiguration.DiagnosticsPageLineSpacing * 2, "ERROR");
                this.currFocus = 0;
