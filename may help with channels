    function setScrollArrows()
    {
        this.downArrowMC._visible = true;
        this.upArrowMC._visible = true;
        if (this.totalPages == 1) 
        {
            this.downArrowMC._visible = false;
        }
        if (this.pageId == 1) 
        {
            this.upArrowMC._visible = false;
        }
        if (this.pageId == this.totalPages) 
        {
            this.downArrowMC._visible = false;
        }
    }

    function pagesInfo()
    {
