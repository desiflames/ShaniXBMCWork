            this.onTierEvent.notify("colorButtonEvent", "draw", "Yellow", "Add to Favorites");
        }
        if (this.grandParentIdx == -1) 
        {
            this.onTierEvent.notify("colorButtonEvent", "disable", "Yellow");
        }
    }

    function displayItemInfo()
    {
        var __reg3 = 0;
