        __reg4 = null;
        __reg3 = null;
    }

    function buttonUnHighlight()
    {
        var __reg2 = this.virtKeyPadBaseMC["keypadBtnBaseMC" + this.currRow + this.currCol].keypadBtnMC.keypadTxtField;
        var __reg4 = __reg2.htmlText;
