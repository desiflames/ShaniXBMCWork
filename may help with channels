        this.barImageMC = __reg2.attachMovie("progressBar", "barImageMC", __reg2.getNextHighestDepth());
        this.barImageMC._x = this.pageConfiguration.speedTestBarX;
        this.barImageMC._y = this.pageConfiguration.speedTestBarY;
