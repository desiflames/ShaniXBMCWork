        __reg1 = __reg6.length;
        while (__reg1 < __reg11) 
        {
            __reg6 = encoding[0] + __reg6;
            ++__reg1;
        }
        return __reg6;
    }

    static function str2rstr_utf8(input)
    {
        var __reg3 = "";
        var __reg2 = -1;
        var __reg1 = undefined;
        var __reg4 = undefined;
