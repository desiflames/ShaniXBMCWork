        parentMC = null;
        contentItem = null;
        maxDuration = null;
    }

    function destroy()
    {
        clearInterval(this.stopVodLoadTimerId);
        clearInterval(this.playbackTimerId);
        clearInterval(this.durationTimerId);
        com.jadoo.as2.JadooPlus.Utils.XmlSocketEvents.resetSocket();
