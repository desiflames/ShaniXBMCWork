        com.jadoo.as2.JadooPlus.Utils.XmlSocketEvents.removeListener("SystemEvent", this, this.onXmlSocketSystemEvent);
        if ((__reg0 = this.viewType) === "ferrariXML:3-TierView.xml") 
        {
