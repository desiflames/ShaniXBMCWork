                {
                    if (this.isNext) 
                    {
                        this.isNext = false;
                        if (this.currentItemIdx >= this.totalItems - 1) 
                        {
                            this.enableView();
                        }
                        else 
                        {
                            this.loadNextItem();
                        }
                    }
                    else if (this.isPrevious) 
                    {
                        this.isPrevious = false;
                        this.loadPreviousItem();
                    }
                }
                else 
                {
                    this.onEnterKey();
                    this.isNext = false;
                    this.isPrevious = false;
                }
            }
            else 
            {
                this.onEnterKey();
                this.isNext = false;
                this.isPrevious = false;
            }
